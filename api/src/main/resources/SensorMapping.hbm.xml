<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.sensorreading">

	<class name="SensorMapping" table="sensor_mapping2">
	    
		<id name="sensor_id" type="java.lang.Integer" column="sensor_id" unsaved-value="0">
			<generator class="increment" />
		</id>

		<property name="sensor_name" type="java.lang.String" column="sensor_name"
			length="19" unique="true" />
		
	</class>
		
</hibernate-mapping>
